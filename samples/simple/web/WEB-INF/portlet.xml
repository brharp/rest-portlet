  <portlet>
    <portlet-name>DEV</portlet-name>
    <portlet-class>
      ca.uoguelph.ccs.portal.rest.RestRemotePortlet
    </portlet-class>
    <expiration-cache>@expiration-cache@</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>@locale@</supported-locale>
    <portlet-info>
      <title>@title@ (DEV)</title>
    </portlet-info> 
    <init-param>
      <name>endpoint</name>
      <value>@dev@</value>
    </init-param>
  </portlet>

